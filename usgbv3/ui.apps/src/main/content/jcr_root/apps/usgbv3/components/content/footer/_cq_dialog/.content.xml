<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Footer Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <basic
                jcr:primaryType="nt:unstructured"
                jcr:title="Basic"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <addMenu
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                class="field-full-width"
                                fieldLabel="Additional Menu">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested="JSON_STORE"
                                    name="./addMenu">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <addMenuTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Additional Menu Title"
                                                    name="./addMenuTitle"/>
                                                <subMenu
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                    class="field-full-width"
                                                    fieldLabel="Additional sub menu section Add Field">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                                        acs-commons-nested="JSON_STORE"
                                                        name="./subMenu">
                                                        <layout
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                                            method="absolute"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <subMenuTitle
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                        fieldLabel="Sub Menu Title"
                                                                        name="./subMenuTitle"
                                                                        required="{Boolean}true"/>
                                                                    <subMenuPath
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                                        fieldLabel="Sub Menu Path"
                                                                        name="./subMenuPath"
                                                                        required="{Boolean}true"
                                                                        rootPath="/content/usgboral"/>
                                                                </items>
                                                            </column>
                                                        </items>
                                                    </field>
                                                </subMenu>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </addMenu>
                            <socialMedia
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                eaem-nested=""
                                fieldLabel="Social Media">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    name="./socialMedia">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <socialIcon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/select"
                                                    fieldLabel="Icon"
                                                    name="./socialIcon">
                                                    <datasource
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="usgbv3/components/structure/common/datasources/socialMediaIcon"/>
                                                </socialIcon>
                                                <externalPath
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Social Media Link"
                                                    name="./externalPath"
                                                    useFixedInlineToolbar="{Boolean}true"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </socialMedia>
                            <copyrights
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Copyrights "
                                name="./copyrights"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <image
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <findreplace
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <subsuperscript
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <spellcheck
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <misctools
                                        jcr:primaryType="nt:unstructured"
                                        features="*">
                                        <specialCharsConfig jcr:primaryType="nt:unstructured">
                                            <chars jcr:primaryType="nt:unstructured">
                                                <copyright
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#169;"/>
                                                <trademark
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#8482;"/>
                                                <registered
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#174;"/>
                                                <emDash
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#8212;"/>
                                                <pound
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#163;"/>
                                                <nbsp
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#160;"/>
                                            </chars>
                                        </specialCharsConfig>
                                    </misctools>
                                    <styles
                                        jcr:primaryType="nt:unstructured"
                                        features="*">
                                        <styles jcr:primaryType="nt:unstructured">
                                            <color-primary-green
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-primary-green"
                                                text="color-primary-green"/>
                                            <color-primary-yellow
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-primary-yellow"
                                                text="color-primary-yellow"/>
                                            <color-secondary-red
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-red"
                                                text="color-secondary-red"/>
                                            <color-secondary-blue
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-blue"
                                                text="color-secondary-blue"/>
                                            <color-secondary-brown
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-brown"
                                                text="color-secondary-brown"/>
                                            <color-secondary-purple
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-purple"
                                                text="color-secondary-purple"/>
                                            <color-secondary-orange
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-orange"
                                                text="color-secondary-orange"/>
                                            <color-secondary-gold
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-gold"
                                                text="color-secondary-gold"/>
                                            <color-secondary-grey
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-grey"
                                                text="color-secondary-grey"/>
                                            <color-secondary-teal
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-teal"
                                                text="color-secondary-teal"/>
                                            <color-white
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-white"
                                                text="color-white"/>
                                            <color-light-grey
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-light-grey"
                                                text="color-light-grey"/>
                                            <color-grey
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-grey"
                                                text="color-grey"/>
                                            <color-dark-grey
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-dark-grey"
                                                text="color-dark-grey"/>
                                            <color-deep-dark-grey
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-deep-dark-grey"
                                                text="color-deep-dark-grey"/>
                                            <color-black
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-black"
                                                text="color-black"/>
                                        </styles>
                                    </styles>
                                    <table
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                    ref="justify"/>
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists"/>
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                                <styles
                                                    jcr:primaryType="nt:unstructured"
                                                    items="styles:getStyles:styles-pulldown"
                                                    ref="styles"/>
                                            </popovers>
                                        </inline>
                                    </cui>
                                </uiSettings>
                                <buttonText/>
                            </copyrights>
                        </items>
                    </column>
                </items>
            </basic>
        </items>
    </content>
</jcr:root>
