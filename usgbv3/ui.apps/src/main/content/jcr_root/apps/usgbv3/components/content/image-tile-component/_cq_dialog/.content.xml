<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Big Small Image Tile"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <basic
                jcr:primaryType="nt:unstructured"
                jcr:title="Basic"
                sling:resourceType="granite/ui/components/foundation/section">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <buttonText/>
                            <styleType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Style Type"
                                name="./styleType">
                                <items jcr:primaryType="nt:unstructured">
                                    <style1
                                        jcr:primaryType="nt:unstructured"
                                        text="Style 1"
                                        value="style1"/>
                                    <style2
                                        jcr:primaryType="nt:unstructured"
                                        text="Style 2"
                                        value="style2"/>
                                </items>
                            </styleType>
                            <bgcolor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="usgb/widgets/colorpicker"
                                classicPaletteType="{Boolean}false"
                                fieldLabel="Background Color"
                                freestylePaletteType="{Boolean}true"
                                name="./bgColor">
                                <mappings jcr:primaryType="nt:unstructured">
                                    <color-primary-green
                                        jcr:primaryType="nt:unstructured"
                                        name="color-primary-green"
                                        value="#009b3a"/>
                                    <color-primary-yellow
                                        jcr:primaryType="nt:unstructured"
                                        name="color-primary-yellow"
                                        value="#fed100"/>
                                    <color-secondary-red
                                        jcr:primaryType="nt:unstructured"
                                        name="color-secondary-red"
                                        value="#c20051"/>
                                    <color-secondary-blue
                                        jcr:primaryType="nt:unstructured"
                                        name="color-secondary-blue"
                                        value="#00a5dd"/>
                                    <color-secondary-brown
                                        jcr:primaryType="nt:unstructured"
                                        name="color-secondary-brown"
                                        value="#845023"/>
                                    <color-secondary-purple
                                        jcr:primaryType="nt:unstructured"
                                        name="color-secondary-purple"
                                        value="#292182"/>
                                    <color-secondary-orange
                                        jcr:primaryType="nt:unstructured"
                                        name="color-secondary-orange"
                                        value="#e85314"/>
                                    <color-secondary-gold
                                        jcr:primaryType="nt:unstructured"
                                        name="color-secondary-gold"
                                        value="#70772a"/>
                                    <color-secondary-grey
                                        jcr:primaryType="nt:unstructured"
                                        name="color-secondary-grey"
                                        value="#4c4d4f"/>
                                    <color-secondary-teal
                                        jcr:primaryType="nt:unstructured"
                                        name="color-secondary-teal"
                                        value="#006870"/>
                                    <color-white
                                        jcr:primaryType="nt:unstructured"
                                        name="color-white"
                                        value="#FFFFFF"/>
                                    <color-light-grey
                                        jcr:primaryType="nt:unstructured"
                                        name="color-light-grey"
                                        value="#CCCCCC"/>
                                    <color-grey
                                        jcr:primaryType="nt:unstructured"
                                        name="color-grey"
                                        value="#999999"/>
                                    <color-dark-grey
                                        jcr:primaryType="nt:unstructured"
                                        name="color-dark-grey"
                                        value="#666666"/>
                                    <color-deep-dark-grey
                                        jcr:primaryType="nt:unstructured"
                                        name="color-deep-dark-grey"
                                        value="#333333"/>
                                    <color-black
                                        jcr:primaryType="nt:unstructured"
                                        name="color-black"
                                        value="#000000"/>
                                </mappings>
                            </bgcolor>
                            <hiddenStyle1/>
                            <contentAlign
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Desktop Content Alignment"
                                name="./contentAlignment">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="usgbv3/components/structure/common/datasources/buttonstyle"/>
                            </contentAlign>
                            <link
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Link Path"
                                name="./link"
                                rootPath="/content/usgboral"/>
                            <buttonStyle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Button Style"
                                name="./buttonStyle">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="usgbv3/components/structure/common/datasources/linkstyle"/>
                            </buttonStyle>
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title"
                                name="./title"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <category
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Category"
                                name="./category"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <imgPath/>
                        </items>
                    </column>
                </items>
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
            </basic>
            <advance
                jcr:primaryType="nt:unstructured"
                jcr:title="Advance"
                sling:resourceType="granite/ui/components/foundation/section">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <opacity
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Opacity"
                                name="./opacity">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="usgbv3/components/structure/common/datasources/opacity"/>
                            </opacity>
                        </items>
                    </column>
                </items>
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
            </advance>
            <mobile
                jcr:primaryType="nt:unstructured"
                jcr:title="Mobile"
                sling:resourceType="granite/ui/components/foundation/section">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Description"
                                name="./mblDescription"
                                useFixedInlineToolbar="{Boolean}true">
                                <buttonText/>
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <image
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <findreplace
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <subsuperscript
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <spellcheck
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <misctools
                                        jcr:primaryType="nt:unstructured"
                                        features="*">
                                        <specialCharsConfig jcr:primaryType="nt:unstructured">
                                            <chars jcr:primaryType="nt:unstructured">
                                                <copyright
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#169;"/>
                                                <trademark
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#8482;"/>
                                                <registered
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#174;"/>
                                                <emDash
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#8212;"/>
                                                <pound
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#163;"/>
                                                <nbsp
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#160;"/>
                                            </chars>
                                        </specialCharsConfig>
                                    </misctools>
                                    <styles
                                        jcr:primaryType="nt:unstructured"
                                        features="*">
                                        <styles jcr:primaryType="nt:unstructured">
                                            <color-primary-green
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-primary-green"
                                                text="color-primary-green"/>
                                            <color-primary-yellow
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-primary-yellow"
                                                text="color-primary-yellow"/>
                                            <color-secondary-red
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-red"
                                                text="color-secondary-red"/>
                                            <color-secondary-blue
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-blue"
                                                text="color-secondary-blue"/>
                                            <color-secondary-brown
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-brown"
                                                text="color-secondary-brown"/>
                                            <color-secondary-purple
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-purple"
                                                text="color-secondary-purple"/>
                                            <color-secondary-orange
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-orange"
                                                text="color-secondary-orange"/>
                                            <color-secondary-gold
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-gold"
                                                text="color-secondary-gold"/>
                                            <color-secondary-grey
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-grey"
                                                text="color-secondary-grey"/>
                                            <color-secondary-teal
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-secondary-teal"
                                                text="color-secondary-teal"/>
                                            <color-white
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-white"
                                                text="color-white"/>
                                            <color-light-grey
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-light-grey"
                                                text="color-light-grey"/>
                                            <color-grey
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-grey"
                                                text="color-grey"/>
                                            <color-dark-grey
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-dark-grey"
                                                text="color-dark-grey"/>
                                            <color-deep-dark-grey
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-deep-dark-grey"
                                                text="color-deep-dark-grey"/>
                                            <color-black
                                                jcr:primaryType="nt:unstructured"
                                                cssName="color-black"
                                                text="color-black"/>
                                        </styles>
                                    </styles>
                                    <table
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                    ref="justify"/>
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists"/>
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                                <styles
                                                    jcr:primaryType="nt:unstructured"
                                                    items="styles:getStyles:styles-pulldown"
                                                    ref="styles"/>
                                            </popovers>
                                        </inline>
                                    </cui>
                                </uiSettings>
                            </description>
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title"
                                name="./mblTitle"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <imgPath/>
                        </items>
                    </column>
                </items>
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
            </mobile>
        </items>
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
    </content>
</jcr:root>
